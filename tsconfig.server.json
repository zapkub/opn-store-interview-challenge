{
    "compilerOptions": {
      "target": "es5",
      "lib": [ ],
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "incremental": true,
      "esModuleInterop": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "strictNullChecks": true
