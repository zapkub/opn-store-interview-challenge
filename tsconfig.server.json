{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "DOM",
        "dom.iterable",
        "esnext"
      ],
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "incremental": true,
      "esModuleInterop": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "strictNullChecks": true
    },
    "include": [
      "next-env.d.ts",
      "apis",
      "index.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  